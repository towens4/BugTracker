@using BugTrackerUI.Models

@model IEnumerable<Application>

<div>
    <select>
        @if(Model.Count() == 0)
        {
           <option>
               <button>Add</button>
           </option>
        }

        @foreach (var item in Model)
        {
            <option>@item.ApplicationName</option>
        }
        
        <option></option>
    </select>
</div>
